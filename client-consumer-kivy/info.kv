MDScreen:
    name: "info"
    MDFloatLayout:
        md_bg_color: 41/255, 34/255, 34/255, 1
        Button:
            pos_hint: {'center_x':0.05, 'center_y':0.95}
            size_hint: (0.4, 0.02)
            font_size: 15
            background_color: 41/255, 34/255, 34/255, 1
            background_normal: ''
            on_release: app.root.current = "user2"
            Image:
                source: "back2.png"
                center_x: self.parent.center_x
                center_y: self.parent.center_y
        MDLabel:
            text: "Products informations."
            pos_hint: {'center_x': 0.5,'center_y':0.95}
            font_size: "25sp"
            font_name: "RSlab"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            halign: 'center'

