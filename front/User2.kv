MDScreen:
    name: "user-interface"
    md_bg_color:41/255, 34/255, 34/255, 1    
    MDLabel:
        text: "points"
        font_name: "Popp"
        font_size: "16sp"
        text_color: [1,1,1,1]
        theme_text_color: 'Custom'
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}
    MDBottomNavigation:
        md_bg_color: 41/255, 34/255, 34/255, 1  
        panel_color: 41/255, 34/255, 34/255, 1
        MDBottomNavigationItem:
            md_bg_color: 41/255, 34/255, 34/255, 1  
            text_color: [1,1,1,1]  
            icon: 'arrow-left-bold-outline'
            icon_color: "white"
            halign: 'center'
            on_tab_press: app.logoutnew()
        MDBottomNavigationItem:
            md_bg_color: 41/255, 34/255, 34/255, 1 
            text: 'Home'
            font_name: "Popp"
            text_color: [1,1,1,1]
            icon: 'home'
            halign: 'left'
            on_tab_press: app.press()
        MDBottomNavigationItem:
            md_bg_color: 41/255, 34/255, 34/255, 1 
            text_color: [1,1,1,1]
            icon_color: 'white'
            icon: 'help'
            halign: 'right'
            on_tab_press: app.root.current = "test" 
    MDTopAppBar:
        left_action_items: [["menu", lambda x: nav.toogle_nav_drawer()]]
        pos_hint: {'center_x':0.5, 'center_y':0.90}
        md_bg_color: 41/255, 34/255, 34/255, 1
        elevation:0
    MDNavigationDrawer:
        id:nav
        elevation:10
        md_bg_color: 41/255, 34/255, 34/255, 1
        MDBoxLayout:
            orientation: 'vertical'
            md_bg_color: 41/255, 34/255, 34/255, 1
            Button:
                text: "My informations"
                halign: 'center'
                font_name: "Popp"
                font_size:"15sp"
                theme_text_color: 'Custom'
                text_color: [1,1,1,1]
                background_color: 41/255, 34/255, 34/255, 1
                background_normal: ""
                size_hint: (0, 0.01)
                pos_hint: {'center_x':0.5, 'center_y': 0.85}
                on_release: app.root.current = "myinfo"

                

