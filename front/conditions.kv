MDScreen:
    name: "condition"
    on_enter: app.toke()   
    MDFloatLayout:
        md_bg_color: 41/255, 34/255, 34/255, 1
        MDLabel:
            text: "Privacy Policy and terms of use."
            pos_hint: {'center_x': 0.45, 'center_y': 0.95}
            theme_text_color: 'Custom'
            halign: 'center'
            text_color: 1,1,1,1
            font_size: "18sp"
            font_name: "Robold"
        MDIconButton:
            icon:'arrow-left'
            icon_color: "white"
            pos_hint: {'center_x':0.05, 'center_y':0.95}
            theme_icon_color: 'Custom'
            on_release: app.root.current = "accueil"
        MDFillRoundFlatButton:
            text: "Accept and continue"
            font_size: "15sp"
            font_name: "Popp"
            pos_hint: {'center_x':0.5, 'center_y':0.15}
            theme_text_color:'Custom'
            elevation: 5
            text_color: 0/255, 0/255, 0/255, 1
            size_hint: (0.6, 0.06)
            md_bg_color: 62/255, 216/255, 133/255, 1
            background_normal:''
            radius: [20]
            on_release:
                app.root.current = "register"


              

